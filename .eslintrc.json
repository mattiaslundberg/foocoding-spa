{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "2019"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "class-methods-use-this": "off",
    "strict": "off",
    "no-plusplus": "off",
    "linebreak-style": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}
